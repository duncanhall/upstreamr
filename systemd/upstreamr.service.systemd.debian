[Unit]
Description=Upstreamr Service
Wants=basic.target
After=basic.target network.target

[Service]
Type=simple
ExecStart=/usr/sbin/upstreamr -p -c /etc/upstreamr/upstreamr.conf
Restart=always

[Install]
WantedBy=multi-user.target
