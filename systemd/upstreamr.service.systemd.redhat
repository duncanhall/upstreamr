[Unit]
Description=Upstreamr Service
After=network.target remote-fs.target nss-lookup.target

[Service]
Type=simple
ExecStart=/usr/sbin/upstreamr -p -c /etc/upstreamr/upstreamr.conf
Restart=always

[Install]
WantedBy=multi-user.target
